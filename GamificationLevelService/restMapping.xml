<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<service consumes="*" name="i5.las2peer.services.gamificationLevelService.GamificationLevelService" path="gamification/levels" produces="text/plain" version="0.1">
  <methods>
    <method httpMethod="put" name="updateLevel" path="{gameId}/{levelNum}" produces="application/json" type="i5.las2peer.restMapper.HttpResponse">
      <parameters>
        <parameter annotation="path" index="0" name="gameId" type="java.lang.String"/>
        <parameter annotation="path" index="1" name="levelNum" type="int"/>
        <parameter annotation="header" index="2" name="Content-Type" type="java.lang.String"/>
        <parameter annotation="content" index="3" name="" type="[B"/>
      </parameters>
    </method>
    <method httpMethod="delete" name="deleteLevel" path="{gameId}/{levelNum}" produces="application/json" type="i5.las2peer.restMapper.HttpResponse">
      <parameters>
        <parameter annotation="path" index="0" name="gameId" type="java.lang.String"/>
        <parameter annotation="path" index="1" name="levelNum" type="int"/>
      </parameters>
    </method>
    <method httpMethod="get" name="getLevelList" path="{gameId}" produces="application/json" type="i5.las2peer.restMapper.HttpResponse">
      <parameters>
        <parameter annotation="path" index="0" name="gameId" type="java.lang.String"/>
        <parameter annotation="query" index="1" name="current" type="int"/>
        <parameter annotation="query" index="2" name="rowCount" type="int"/>
        <parameter annotation="query" index="3" name="searchPhrase" type="java.lang.String"/>
      </parameters>
    </method>
    <method httpMethod="post" name="createLevel" path="{gameId}" produces="application/json" type="i5.las2peer.restMapper.HttpResponse">
      <parameters>
        <parameter annotation="path" index="0" name="gameId" type="java.lang.String"/>
        <parameter annotation="header" index="1" name="Content-Type" type="java.lang.String"/>
        <parameter annotation="content" index="2" name="" type="[B"/>
      </parameters>
    </method>
    <method httpMethod="get" name="getlevelWithNum" path="{gameId}/{levelNum}" produces="application/json" type="i5.las2peer.restMapper.HttpResponse">
      <parameters>
        <parameter annotation="path" index="0" name="gameId" type="java.lang.String"/>
        <parameter annotation="path" index="1" name="levelNum" type="int"/>
      </parameters>
    </method>
  </methods>
</service>
